<!DOCTYPE html>
	<head>
		<meta charset="UTF-8">
		<title>{% block title %}{% endblock %}</title>
		{% include "RenaissanceWebBundle:Layout:stylesheets.html.twig" %}
		{% block stylesheets %}
		{% endblock %}
	</head>
	<body>
		<div id="bodyWrapper">
			{% block navbar %}
			{% if no_navbar is defined%}
			{% else %}
				{% include "RenaissanceWebBundle:Layout:nav_normal.html.twig" %}
			{% endif %}
			{% endblock %}
			{% block content %}{% endblock %}
			{% include "RenaissanceWebBundle:Layout:base_bar.html.twig" %}
			{% include "RenaissanceWebBundle:Layout:footer.html.twig" %}
		</div>
	{% include "RenaissanceWebBundle:Layout:scripts.html.twig" %}
	{% block scripts %}{% endblock %}
	</body>
</html>